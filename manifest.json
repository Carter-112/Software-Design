      "protocol": "web+javacode",
      "url": "/?search=%s"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "background_sync": {
    "sync_interval": "periodic",
    "periodic_sync": true
  },
  "permissions_policy": {
    "geolocation": ["self"]
  },
